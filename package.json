    "description": "ðŸ†– Angular Dynamic Compile - Convert strings to Angular components",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --aot=false",
        "build": "ng build",
        "test": "grunt",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build-lib": "ng build --prod --project angular-compile && cp README.md ./dist/angular-compile && cp LICENSE ./dist/angular-compile && node node_modules/corifeus-builder/src/utils/angular/post-lib-build.js",
        "build-app-info": "Have to disable AOT for this build",
        "build-app": "ng build --prod --source-map=false --extract-css=true --output-hashing=all --configuration=production --deploy-url=/ --base-href=/ --aot=false --build-optimizer=false --optimization=true"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/patrikx3/angular-compile.git"
    },
    "keywords": [
        "p3x",
        "angular",
        "ng",
        "compile",
        "html",
        "angular",
        "dynamic",
