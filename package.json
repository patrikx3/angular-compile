{
    "name": "p3x-angular-compile",
    "version": "2020.10.127",
    "corifeus": {
        "prefix": "p3x-",
        "publish": true,
        "type": "p3x",
        "code": "Logico",
        "nodejs": "v12.19.0",
        "opencollective": false,
        "reponame": "angular-compile",
        "build": true,
        "cdn": true,
        "angular": "10.1.5",
        "publish-location": "./dist/angular-compile"
